<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>Start eddress</th>
      <th>End address</th>
      <th>Resa date</th>
      <th>Price</th>
      <th>Client</th>
      <th>Driver</th>
      <th>Note</th>
      <th>Comment</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.start_eddress %></td>
        <td><%= course.end_address %></td>
        <td><%= course.resa_date %></td>
        <td><%= course.price %></td>
        <td><%= course.client_id %></td>
        <td><%= course.driver_id %></td>
        <td><%= course.note %></td>
        <td><%= course.comment %></td>
        <td><%= course.status %></td>
        <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
